{
  "private": true,
  "repository": "https://github.com/aura-glitch/aglith-ds.git",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "auto": "^10.36.5",
    "eslint": "7.32.0",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "aglitch-ds",
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  },
  "version": "0.0.1"
}
